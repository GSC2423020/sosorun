{
  "name": "sosorun",
  "private": true,
  "packageManager": "pnpm@9.15.9",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "pnpm -r --parallel run dev",
		"dev:web": "pnpm --filter @sosorun/web dev",
		"dev:api": "pnpm --filter @sosorun/api start:dev",
		
		"build": "pnpm -r run build",
		"start": "pnpm -r run start"
  }
}
