# =========================================
# SOSORUN .gitignore (Laravel + Docker + Vite)
# =========================================

# =========================
# OS / Editor
# =========================
.DS_Store
Thumbs.db
.vscode/
.idea/
*.iml

# =========================
# Env (never commit secrets)
# =========================
.env
.env.*.local

# =========================
# Laravel
# =========================
/vendor/
/storage/*.key
/storage/app/*
!/storage/app/.gitignore
/storage/framework/cache/*
/storage/framework/sessions/*
/storage/framework/views/*
/storage/logs/*
/bootstrap/cache/*

# =========================
# Node / Vite
# =========================
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
.pnpm-store/
.vite/

# =========================
# Build outputs / caches
# =========================
public/build/
*.log

# =========================
# Docker (local volumes / temp)
# =========================
*.pid
